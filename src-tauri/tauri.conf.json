{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ZError",
  "version": "2.0.2",
  "identifier": "com.zerror.app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist",
    "beforeBuildCommand": "npx vite build"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null,
      "capabilities": ["default", "url-content"]
    }
  },
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "icon": [
      "icons/favicon_128x128.ico",
      "icons/favicon_128x128.ico",
      "icons/favicon_128x128.ico",
      "icons/favicon_256x256.ico",
      "icons/favicon_256x256.ico",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "icons/**/*",
      "../src/assets/images/**/*",
      "../public/assets/images/**/*"
    ],
    "windows": {
      "wix": {
        "language": "zh-CN"
      },
      "nsis": null
    }
  },
  "plugins": {}
}
